{
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"hosting": {
		"public": "build",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"cleanUrls": true,
		"site": "pyau",
		"rewrites": [
			{
				"source": "**",
				"destination": "/200.html"
			}
		]
	},
	"storage": {
		"rules": "storage.rules"
	}
}
